# Server Configuration
PORT=3000

# API Key for validating incoming requests (FROM Rails backend)
API_KEY=your-api-key-here

# Admin API Configuration (FOR making requests TO Rails backend)
ADMIN_API_KEY=your-admin-api-key-here
ADMIN_API_BASE_URL=https://your-admin-api.com

# Browserbase credentials
BROWSERBASE_API_KEY=your-browserbase-api-key-here
BROWSERBASE_PROJECT_ID=your-browserbase-project-id-here

# Gemini API (used by Stagehand)
GEMINI_API_KEY=your-gemini-api-key-here

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret
