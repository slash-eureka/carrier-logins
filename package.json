{
  "name": "carrier-logins",
  "version": "1.0.0",
  "type": "module",
  "description": "Browser automation service for downloading insurance carrier commission statements",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node --import=./dist/instrument.js dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "workflow": "tsx cli/run-workflow.ts",
    "lint": "eslint src cli --ext .ts",
    "lint:fix": "eslint src cli --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"cli/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"cli/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@browserbasehq/stagehand": "^2.5.2",
    "@sentry/node": "^8.47.0",
    "@sentry/profiling-node": "^8.47.0",
    "axios": "^1.7.2",
    "cloudinary": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
